startshape SHAPES {color alpha}

rule SHAPES { 
SHAPE {sat =1 b =1 alpha =1}
SHAPE {sat =1 b =1 alpha =1 r 90}
SHAPE {sat =1 b =1 alpha =1 r 180}
SHAPE {sat =1 b =1 alpha =1 r 270}
}

rule SHAPE { A{ } SHAPE{x 2.1 s 0.9 hue 5 r 20}}

rule A { B { } A { x 1 s 0.9 r 55} }

rule B { CIRCLE{} B { x 0.002 y 0.002 s 0.99 sat -0.01 alpha -0.01}}

